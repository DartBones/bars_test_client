<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane
        minWidth="315.0"
        prefWidth="800.0"
        prefHeight="400.0"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.dartbony.bars_test_client.BarsClientController">
    <TableView
            fx:id="documentTable"
            AnchorPane.bottomAnchor="60.0"
            AnchorPane.leftAnchor="10.0"
            AnchorPane.rightAnchor="10.0"
            AnchorPane.topAnchor="40.0">
        <columns>
            <TableColumn fx:id="number" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0" text="Номер" />
            <TableColumn fx:id="date" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0" text="Дата" />
            <TableColumn fx:id="updateDate" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0" text="Дата последнего обновления" />
            <TableColumn fx:id="actual" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0" text="Актуальность" editable="false" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>